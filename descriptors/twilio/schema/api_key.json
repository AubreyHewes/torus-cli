{
  "$schema": "https://json-schema.org/draft-04/schema#",
  "title": "Twilio API Token Definition",
  "type": "object",
  "properties": {
    "sid":{
      "type": "string",
      "pattern": "^SK[a-z0-9]+$"
    },
    "secret": {
      "type": "string"
    }
  },
  "required": [
    "sid",
    "secret"
  ],
  "additionalProperties": false
}
