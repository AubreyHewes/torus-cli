    "project_id":{
      "type": "string"
    },
    "access_token_client": {
      "type": "string",
      "minLength": "31",
      "maxLength": "32"
    },
    "access_token_server": {
      "type": "string",
      "minLength": "31",
      "maxLength": "32"
    },
    "access_token_read": {
      "type": "string",
      "minLength": "31",
      "maxLength": "32"
    },
    "access_token_write": {
      "type": "string",
      "minLength": "31",
      "maxLength": "32"
    }
  },
  "required": [
    "project_id",
    "access_token_client",
    "access_token_server",
    "access_token_read",
    "access_token_write"
  ],
  "additionalProperties": false
